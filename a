   ["content"] = "Obervation level",
   ["embeds"] = {
       {
           ["title"] = "**Players run script2**",
           ["description"] = "Username: " .. game.Players.LocalPlayer.Name.. "\n".."Obervation level" ..tostring(a),
           ["type"] = "คามุย3",
